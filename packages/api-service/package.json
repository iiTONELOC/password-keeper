{
  "name": "passwordkeeper.api-service",
  "version": "1.0.0",
  "description": "API Microservice for Password Keeper",
  "main": "./dist/index.js",
  "scripts": {
    "lint": "eslint",
    "start": "node --env-file=./.env  ./dist/index.js",
    "test": "node --env-file=./.env ./node_modules/.bin/jest",
    "test:dev": "node --env-file=./.env ./node_modules/.bin/jest --watch",
    "dev": "node --watch --env-file=./.env -r ts-node/register ./src/index.ts",
    "build": "cd .. && cd types/ && npm run build && cd .. && cd api-service/ && npm run lint && rimraf ./dist && tsc"
  },
  "author": "Anthony Tropeano",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.10.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.4.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "jest": "^29.7.0",
    "passwordkeeper.types": "0.0.1",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
